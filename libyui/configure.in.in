## libyui configure.in.in

## initialize
@YAST2-INIT-COMMON@
AC_SYS_LARGEFILE

@YAST2-INIT-PROGRAM@


# for historic reasons this value is called SUSEVERSION
SUSEVERSION=`echo $VERSION | cut -d . -f 1,2`
AC_SUBST(SUSEVERSION)


AX_CHECK_DOCBOOK
CREATE_PKGCONFIG=true
## some common checks
@YAST2-CHECKS-COMMON@
@YAST2-CHECKS-PROGRAM@

@YAST2-OUTPUT@
